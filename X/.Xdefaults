URxvt*foreground : White
URxvt*background : [95]Black
URxvt*depth: 32
URxvt*saveLines: 100000

! fonts
! run "fc-list" for a list of available fonts
URxvt*font: xft:xos4 Terminus:pixelsize=16

! extensions: font size and URL matcher
URxvt.perl-ext-common: font-size,matcher
URxvt.urlLauncher: qutebrowser
URxvt.matcher.button: 1

! save lines
URxvt.saveLines: 50000

! no scrollbars
URxvt*scrollBar: False
URxvt*scrollWithBuffer: False

! k and j to scroll up and down
URxvt.keysym.Meta-k: command:\033]720;1\007
URxvt.keysym.Meta-j: command:\033]721;1\007
